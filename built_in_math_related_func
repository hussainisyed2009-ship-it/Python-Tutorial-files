
x = 3.14
y = -4
z = 5
d = 3

result = round(x) #rounds to nearest whole number
print(result)

result_y = abs(y) #turns postive
print(result_y)

result_z = pow(z, 3) #exponentiation
print(result_z)

max_result = max(x, y, z, d) #finds the max value
print(max_result)

min_result = min(x, y, z, d) #finds the min value
print(min_result)
