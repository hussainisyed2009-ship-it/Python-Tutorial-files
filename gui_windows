from tkinter import *

# widgets = GUI elements: buttons, textbooks, labels, images
# windows = serves as a container to hold or contain these widgets

window = Tk() #isntantiate an instance of a window
window.geometry("420x420")
window.title("Taha testing GUI")


#icon = PhotoImage(file='speed.jpg')
#window.iconphoto(True, icon)
window.config(background="#5cfcff")

window.mainloop() # places a window on screen, listens for events
