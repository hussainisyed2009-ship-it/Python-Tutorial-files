
phone_number = input("Enter your phone number: ")

phone_number.count("-") # count the number of dashes in the phone number

phone_number.replace("-", "") # remove dashes from the phone number

print(help(str)) # display all string methods
print(dir(str)) # display all attributes and methods of the string class
print(dir(int)) # display all attributes and methods of the integer class
print(dir(float)) # display all attributes and methods of the float class
print(dir(bool)) # display all attributes and methods of the boolean class
