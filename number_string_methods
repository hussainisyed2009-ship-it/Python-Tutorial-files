print(dir(tuple)) # display all attributes and methods of the tuple class
print(dir(complex)) # display all attributes and methods of the complex class
print(dir(object)) # display all attributes and methods of the object class
print(dir(type)) # display all attributes and methods of the type class
print(dir(bytes)) # display all attributes and methods of the bytes class
print(dir(bytearray)) # display all attributes and methods of the bytearray class
print(dir(memoryview)) # display all attributes and methods of the memoryview class
print(dir(frozenset)) # display all attributes and methods of the frozenset class
print(dir(range)) # display all attributes and methods of the range class
print(dir(slice)) # display all attributes and methods of the slice class
print(dir(property)) # display all attributes and methods of the property class
print(dir(staticmethod)) # display all attributes and methods of the staticmethod class
print(dir(classmethod)) # display all attributes and methods of the classmethod class
